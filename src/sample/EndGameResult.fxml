<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Game.EndGameResult">
   <children>
      <ImageView fx:id="bckgrnd" fitHeight="523.0" fitWidth="415.0" pickOnBounds="true">
         <image>
            <Image url="@assets/backdrop.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="partyimage" fitHeight="171.0" fitWidth="415.0" layoutY="-13.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/party.png" />
         </image>
      </ImageView>
      <Text fill="WHITE" layoutX="82.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Congratulations !!!" textAlignment="CENTER">
         <font>
            <Font size="31.0" />
         </font>
      </Text>
      <Text fx:id="winnerText" fill="WHITE" layoutX="89.0" layoutY="322.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player1" textAlignment="CENTER" wrappingWidth="252.82861328125">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Text fx:id="loserText" fill="WHITE" layoutX="73.0" layoutY="387.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player2" textAlignment="CENTER" wrappingWidth="269.3332214355469">
         <font>
            <Font size="19.0" />
         </font>
      </Text>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="108.0" layoutY="133.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/winner.gif" />
         </image>
      </ImageView>
      <Button fx:id="restartGameButton" layoutX="152.0" layoutY="462.0" mnemonicParsing="false" onAction="#playAgain" onMouseEntered="#setRestartGameButtonSkin" onMouseExited="#removeRestartGameButtonSkin" prefHeight="35.0" prefWidth="111.0" text="PLAY AGAIN">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="stopPlayingButton" layoutX="152.0" layoutY="414.0" mnemonicParsing="false" onAction="#stopPlaying" onMouseEntered="#setStopPlayingButtonSkin" onMouseExited="#removeStopPlayingButtonSkin" prefHeight="35.0" prefWidth="111.0" text="STOP PLAYING">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <ImageView fitHeight="506.0" fitWidth="407.0" layoutX="2.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/celebration.gif" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
